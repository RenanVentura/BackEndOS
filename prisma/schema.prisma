generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String   @id @default(uuid()) @map("_id")
  email        String   @unique
  name         String
  password     String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
  filial       String
  levelUser    Int
  statusDelete Boolean
  costCenter   String
}

model Solicitation {
  id                      String   @id @default(uuid()) @map("_id")
  numSol                  Int
  userName                String
  filial                  String
  urgency                 String
  createdAt               DateTime @default(now())
  updatedAt               DateTime @updatedAt
  atendedAt               DateTime?
  categoryEquipment       String
  tagEquipment            String
  equipment               String
  categoryService         String
  description             String
  status                  String
  statusDelete            Boolean
  costCenter              String
}

model SolicitationHistoric {
  id                      String   @id @default(uuid()) @map("_id")
  numSol                  Int
  userName                String
  filial                  String
  urgency                 String
  createdAt               DateTime @default(now())
  updatedAt               DateTime @updatedAt
  atendedAt               DateTime?
  categoryEquipment       String
  tagEquipment            String
  equipment               String
  categoryService         String
  description             String
  status                  String
  statusDelete            Boolean
  costCenter              String
}

model categoryEquipment {
  id                String   @id @default(uuid()) @map("_id")
  name              String
  statusDelete      Boolean
}

model Equipment {
  id                String   @id @default(uuid()) @map("_id")
  tagEquipment        String
  name              String
  filial            String
  statusDelete      Boolean
  categoryEquipment String
}

model filial {
  id                String   @id @default(uuid()) @map("_id")
  name              String
  statusDelete      Boolean
}